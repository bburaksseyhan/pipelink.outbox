<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>Pipelink.Outbox</id>
        <version>1.0.13</version>
        <authors>Pipelink</authors>
        <owners>Pipelink</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A .NET library for implementing the outbox pattern with Entity Framework Core. This package provides a reliable way to handle message publishing in distributed systems, ensuring that messages are not lost even if the system fails.</description>
        <releaseNotes>Added comprehensive README files with implementation details and test results. Improved documentation and examples.</releaseNotes>
        <copyright>Copyright Â© 2025</copyright>
        <tags>Outbox Pattern EntityFrameworkCore EFCore Distributed-Systems Message-Queue</tags>
        <repository type="git" url="https://github.com/seyhanb/pipelink.outbox" />
        <dependencies>
            <group targetFramework="net8.0">
                <dependency id="Microsoft.EntityFrameworkCore" version="8.0.0" />
            </group>
        </dependencies>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </metadata>
    <files>
        <file src="bin/Release/net8.0/Pipelink.Outbox.dll" target="lib/net8.0" />
        <file src="pipelink-nuget-icon.png" target="icon.png" />
        <file src="README.md" target="/" />
        <file src="LICENSE" target="/" />
    </files>
</package> 